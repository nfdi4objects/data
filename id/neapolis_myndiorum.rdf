   <nmo:Mint rdf:about="http://nomisma.org/id/neapolis_myndiorum">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">Neapolis Myndiorum</skos:prefLabel>
      <skos:broader rdf:resource="http://nomisma.org/id/caria"/>
      <dcterms:isPartOf rdf:resource="http://nomisma.org/id/greek_numismatics"/>
      <skos:definition xml:lang="en">The mint posited for the ancient site of Neapolis Myndiorum in Caria.  The coinage so attributed by Head (BMC), seems rather to belong to Neapolis ad Harpasum. See F. Delrieux, Les monnaies des cités grecques de la basse vallée de l'Harpasos en Carie (IIe siècle a.C. - IIIe siècle p. C.) (Bordeaux, 2008).</skos:definition>
      <skos:inScheme rdf:resource="http://nomisma.org/id/"/>
      <skos:changeNote rdf:resource="http://nomisma.org/id/neapolis_myndiorum#provenance"/>
      <skos:closeMatch rdf:resource="http://nomisma.org/id/neapolis_ad_harpasum"/>
   </nmo:Mint>
