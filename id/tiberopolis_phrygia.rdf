   <nmo:Mint rdf:about="http://nomisma.org/id/tiberopolis_phrygia">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">Tiberopolis</skos:prefLabel>
      <skos:definition xml:lang="en">The mint at the ancient site of Tiberopolis in north-western Phrygia. The site has not been identified yet. Civic coins minted from Trajan to Tranquillina. City famously destroyed by an earthquake under Tiberius and given his name following reconstruction. Compare H. von Aulock, Münzen und Städte Phrygiens vol II. Istanbuler Mitteilungen Beiheft 27 (1987) 37-42; RPC VII-1 p. 163.</skos:definition>
      <dcterms:isPartOf rdf:resource="http://nomisma.org/id/roman_provincial_numismatics"/>
      <skos:inScheme rdf:resource="http://nomisma.org/id/"/>
      <skos:changeNote rdf:resource="http://nomisma.org/id/tiberopolis_phrygia#provenance"/>
      <skos:exactMatch rdf:resource="https://ikmk.smb.museum/ndp/ort/2127"/>
   </nmo:Mint>
